<header>
  <div class="headercontents">
    <%=image_tag("logo.svg", alt:"idealogo",class:"idealogo")%>
    <%=image_tag("titlelogo.svg", alt:"ideatitle",class:"ideatitle")%>
    <div class="headerleftWords">
      <%= link_to "HOME", root_path, class:"user_links" %>
      <%= link_to "ABOUT", "#", class:"user_links" %>
      <%if logged_in?%>
      <%= link_to "USER", student_path(current_student), class:"user_links" %>
      <%end%>
    </div>
    <div class="headerrightWords">
      <%if logged_in?%>
      <%= link_to "logout",logout_path, method: :delete, class:"user_links" %>
      <%else%>
      <%= link_to "login", login_path, class:"login_link" %>
      <%= link_to "signup", signup_path, class:"signup_link" %>
      <%end%>
    </div>
  </div>


</header>
